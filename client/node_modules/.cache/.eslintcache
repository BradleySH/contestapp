ent\"\n\nfunction App() {\n  const {\n    token,\n    user: {\n      role\n    }\n  } = useContext(UserContext)\n  return (\n    <div className=\"App\">\n      <Switch>\n      <Route\n          exact path='/'\n          render={() => {\n            if(token){\n              if(role === 'general' || 'commissioner'){\n                return <Redirect to='/profile' />\n              }\n            }\n            else {\n              return <GeneralAuth />\n            }\n          }}\n        />\n        <Route \n          path=\"/admin\"\n          render={() => {if(token && role === 'admin'){\n            console.log(role)\n            return <Redirect to='/profile' />\n          }\n          else {\n            console.log(role)\n            return <AdminAuth />\n          }\n          }}\n        />\n      </Switch>\n\n      {token ? <Header /> : null}\n        <main style={{height: token ? '100vh' : 0, width: '100vw', scrollBehavior: \"smooth\", overflow: \"scroll\"}}>\n        <Switch>\n          <ProtectedRoute\n            path='/profile'\n            component={Profile}\n            redirectTo='/'\n            token={token}\n          />\n          <ProtectedRoute\n            path='/adminclient'\n            component={AdminClient}\n            redirectTo='/'\n            token={token}\n          />\n          <ProtectedRoute\n            path='/client'\n            component={Client}\n            redirectTo='/'\n            token={token}\n          />\n          <ProtectedRoute\n            path='/createclient'\n            component={CreateClient}\n            redirectTo='/'\n            token={token}\n          />\n          <ProtectedRoute\n            path='/team'\n            component={Team}\n            redirectTo='/'\n            token={token}\n          />\n          <ProtectedRoute\n            path='/createteam'\n            component={CreateTeam}\n            redirectTo='/'\n            token={token}\n          />\n          <ProtectedRoute\n   