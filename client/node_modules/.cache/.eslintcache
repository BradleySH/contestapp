<p>*1st</p>\n        </div>\n        </div>\n        \n        <p className=\"team-title\">Team Members</p>\n        <div className=\"team-members\">\n          <div className=\"coach\">\n            <StarsIcon style={{ color: \"gray\", height: \"40px\", width: \"40px\"}} />\n            <p>Captain</p>\n          </div>\n          <div className=\"member1\">\n            <AccountCircleIcon style={{ color: \"gray\",height: \"40px\", width: \"40px\"}} />\n            <p>Name</p>\n          </div>\n          <div className=\"member2\">\n            <AccountCircleIcon style={{ color: \"gray\",height: \"40px\", width: \"40px\"}} />\n            <p>Name</p>\n          </div>\n          <div className=\"member2\">\n            <AccountCircleIcon style={{ color: \"gray\",height: \"40px\", width: \"40px\"}} />\n            <p>Name</p>\n          </div>\n          <div className=\"member3\">\n            <AccountCircleIcon style={{ color: \"gray\",height: \"40px\", width: \"40px\"}} />\n            <p>Name</p>\n          </div>\n          <div className=\"member4\">\n            <AccountCircleIcon style={{ color: \"gray\",height: \"40px\", width: \"40px\"}} />\n            <p>Name</p>\n          </div>\n          <div className=\"member5\">\n          < AccountCircleIcon style={{ color: \"gray\",height: \"40px\", width: \"40px\"}} />\n          <p>Name</p>\n          </div>\n        </div>\n      </div>\n    </div>\n    <p className=\"team-standing\">Current Standings</p>\n    <div className=\"team-info\">\n      <div className=\"graph\">\n        <div className=\"second\">\n          <div className=\"two\">\n          <p>Team Name</p>\n            <p># Points</p>\n            <p>2nd</p>\n          </div>\n        </div>\n        <div className=\"first\">\n          <div className=\"one\">\n            <p>Team Name</p>\n            <p># Points</p>\n            <p>1st</p>\n          </div> \n        </div>\n        <div className=\"third\">\n          <div className=\"three\">\n            <p>Team Name</p>\n            <p># Points</p>\n            <p>3rd</p>\n          </div>\n        </div>\n      </div>\n      <div className=\"team-ranking\">\n        <div className=\"team1\">\n          <p>TeamName</p>\n          <div className=\"avatar\"></div>\n          <label>rank</label>\n          <p># Points</p>\n        </div>\n        <div className=\"team2\">\n          <p>TeamName</p>\n          <div className=\"avatar\"></div>\n          <label>rank</label>\n          <p># Points</p>\n        </div>\n        <div className=\"team3\">\n          <p>TeamName</p>\n          <div className=\"avatar\"></div>\n          <label>rank</label>\n          <p># Points</p>\n        </div>\n        <div className=\"team4\">\n          <p>TeamName</p>\n          <div className=\"avatar\"></div>\n          <label>rank</label>\n          <p># Points</p>\n        </div>\n      </div>\n    </div>\n    <div className=\"btn-logout\">\n      <button className=\"logout\" onClick={logout}>Logout</button>\n    </div>\n    </>\n  )\n}\n\nexport default UserProfile","/Users/markgw/Desktop/dev/contestapp/client/pinion-games/src/pages/AdminAuth.js",[],"/Users/markgw/Desktop/dev/contestapp/client/pinion-games/src/components/GeneralAuthLogin.js",[],"/Users/markgw/Desktop/dev/contestapp/client/pinion-games/src/components/GeneralAuthForm.js",[],"/Users/markgw/Desktop/dev/contestapp/client/pinion-games/src/components/AdminAuthForm.js",[],"/Users/markgw/Desktop/dev/contestapp/client/pinion-games/src/components/ClientTag.js",[],"/Users/markgw/Desktop/dev/contestapp/client/pinion-games/src/pages/Client.js",[],"/Users/markgw/Desktop/dev/contestapp/client/pinion-games/src/components/TeamTag.js",[],"/Users/markgw/Desktop/dev/contestapp/client/pinion-games/src/pages/Team.js",["149","150"],"import { useState, useEffect, useContext } from \"react\"\nimport { Link } from \"react-router-dom\";\nimport { useLocation } from \"react-router\"\nimport { UserContext } from \"../context/Use